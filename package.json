{
  "name": "@balena/abstract-sql-compiler",
  "version": "7.5.0",
  "description": "A translator for abstract sql into sql.",
  "main": "out/AbstractSQLCompiler.js",
  "types": "out/AbstractSQLCompiler.d.ts",
  "scripts": {
    "pretest": "npm run prepare",
    "test": "mocha",
    "posttest": "npm run lint",
    "prepublish": "require-npm4-to-publish",
    "prepare": "tsc --project ./tsconfig.build.json",
    "lint": "balena-lint --typescript --fix -e js -e ts src/ test/ && tsc --noEmit && tsc --noEmit --project tsconfig.js.json"
  },
  "repository": "https://github.com/balena-io-modules/abstract-sql-compiler.git",
  "author": "",
  "dependencies": {
    "@balena/sbvr-types": "^3.1.3",
    "@types/lodash": "^4.14.165",
    "@types/node": "^10.17.49",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@balena/lf-to-abstract-sql": "^4.1.1",
    "@balena/lint": "^5.3.0",
    "@balena/odata-parser": "^2.2.1",
    "@balena/sbvr-parser": "^1.1.1",
    "@resin/odata-to-abstract-sql": "^3.3.0",
    "@types/chai": "^4.2.14",
    "@types/common-tags": "^1.8.0",
    "@types/mocha": "^8.2.0",
    "chai": "^4.2.0",
    "common-tags": "^1.8.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
    "require-npm4-to-publish": "^1.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "balena-lint --typescript --fix"
    ],
    "*.ts": [
      "balena-lint --typescript --fix"
    ]
  },
  "mocha": {
    "reporter": "spec",
    "recursive": true,
    "require": "ts-node/register/transpile-only",
    "bail": true,
    "_": [
      "test/**/*.ts",
      "test/**/*.js"
    ]
  }
}
