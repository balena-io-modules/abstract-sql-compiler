{
  "name": "@balena/abstract-sql-compiler",
  "version": "7.24.0",
  "description": "A translator for abstract sql into sql.",
  "main": "out/AbstractSQLCompiler.js",
  "types": "out/AbstractSQLCompiler.d.ts",
  "scripts": {
    "pretest": "npm run prepare",
    "test": "mocha",
    "posttest": "npm run lint",
    "prepublish": "require-npm4-to-publish",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\" && tsc --project ./tsconfig.build.json",
    "lint-fix": "balena-lint --fix -e js -e ts src/ test/",
    "lint": "balena-lint -e js -e ts src/ test/ && tsc --noEmit && tsc --noEmit --project tsconfig.js.json"
  },
  "repository": "https://github.com/balena-io-modules/abstract-sql-compiler.git",
  "author": "",
  "dependencies": {
    "@balena/sbvr-types": "^3.4.18",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@balena/lf-to-abstract-sql": "^4.6.0",
    "@balena/lint": "^6.2.1",
    "@balena/odata-parser": "^2.4.2",
    "@balena/odata-to-abstract-sql": "^5.7.2",
    "@balena/sbvr-parser": "^1.4.3",
    "@types/chai": "^4.3.4",
    "@types/common-tags": "^1.8.1",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.11",
    "chai": "^4.3.7",
    "common-tags": "^1.8.2",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "mocha": "^10.1.0",
    "require-npm4-to-publish": "^1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.js": [
      "balena-lint --fix"
    ],
    "*.ts": [
      "balena-lint --fix"
    ]
  },
  "mocha": {
    "reporter": "spec",
    "recursive": true,
    "require": "ts-node/register/transpile-only",
    "bail": true,
    "_": [
      "test/**/*.ts",
      "test/**/*.js"
    ]
  },
  "versionist": {
    "publishedAt": "2023-01-16T14:40:51.044Z"
  }
}
