{
  "name": "@balena/abstract-sql-compiler",
  "version": "7.20.13",
  "description": "A translator for abstract sql into sql.",
  "main": "out/AbstractSQLCompiler.js",
  "types": "out/AbstractSQLCompiler.d.ts",
  "scripts": {
    "pretest": "npm run prepare",
    "test": "mocha",
    "posttest": "npm run lint",
    "prepublish": "require-npm4-to-publish",
    "prepare": "tsc --project ./tsconfig.build.json",
    "lint-fix": "balena-lint --fix -e js -e ts src/ test/",
    "lint": "balena-lint -e js -e ts src/ test/ && tsc --noEmit && tsc --noEmit --project tsconfig.js.json"
  },
  "repository": "https://github.com/balena-io-modules/abstract-sql-compiler.git",
  "author": "",
  "dependencies": {
    "@balena/sbvr-types": "^3.4.7",
    "@types/lodash": "^4.14.182",
    "@types/node": "^10.17.60",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@balena/lf-to-abstract-sql": "^4.5.1",
    "@balena/lint": "^6.2.1",
    "@balena/odata-parser": "^2.2.9",
    "@balena/sbvr-parser": "^1.4.1",
    "@resin/odata-to-abstract-sql": "^3.3.0",
    "@types/chai": "^4.3.4",
    "@types/common-tags": "^1.8.1",
    "@types/mocha": "^10.0.0",
    "chai": "^4.3.7",
    "common-tags": "^1.8.2",
    "husky": "^4.3.8",
    "lint-staged": "^13.0.3",
    "mocha": "^10.1.0",
    "require-npm4-to-publish": "^1.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "balena-lint --fix"
    ],
    "*.ts": [
      "balena-lint --fix"
    ]
  },
  "mocha": {
    "reporter": "spec",
    "recursive": true,
    "require": "ts-node/register/transpile-only",
    "bail": true,
    "_": [
      "test/**/*.ts",
      "test/**/*.js"
    ]
  },
  "versionist": {
    "publishedAt": "2022-11-17T04:28:32.390Z"
  }
}
