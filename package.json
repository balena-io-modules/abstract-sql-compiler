{
  "name": "@balena/abstract-sql-compiler",
  "version": "10.2.9",
  "description": "A translator for abstract sql into sql.",
  "type": "commonjs",
  "main": "out/AbstractSQLCompiler.js",
  "types": "out/AbstractSQLCompiler.d.ts",
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "pretest": "npm run build",
    "test": "mocha",
    "posttest": "npm run lint",
    "prepare": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module && npm run build",
    "lint-fix": "balena-lint --fix src/ test/",
    "lint": "balena-lint src/ test/ && tsc --noEmit"
  },
  "repository": "https://github.com/balena-io-modules/abstract-sql-compiler.git",
  "author": "",
  "peerDependencies": {
    "@balena/sbvr-types": "^7.1.0 || ^8.0.0 || ^9.0.2"
  },
  "devDependencies": {
    "@balena/lf-to-abstract-sql": "^5.0.4",
    "@balena/lint": "^9.0.0",
    "@balena/odata-parser": "^3.1.2",
    "@balena/odata-to-abstract-sql": "^7.2.0",
    "@balena/sbvr-parser": "^1.4.9",
    "@types/chai": "^4.3.20",
    "@types/common-tags": "^1.8.4",
    "@types/lodash": "^4.17.16",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.30",
    "chai": "^4.5.0",
    "common-tags": "^1.8.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "lodash": "^4.17.21",
    "mocha": "^11.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ]
  },
  "mocha": {
    "reporter": "spec",
    "recursive": true,
    "require": "ts-node/register/transpile-only",
    "bail": true,
    "_": "test/**/*.ts"
  },
  "engines": {
    "node": ">=20.14.0",
    "npm": ">=10.7.0"
  },
  "versionist": {
    "publishedAt": "2025-04-07T16:16:45.047Z"
  }
}
